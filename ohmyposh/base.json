{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\udb85\udd45 ",
    "foreground": "#c4a7e7",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "@{{ .UserName }} ",
          "foreground": "#c4a7e7",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": "\udb84\udf76 '{{ .Path }}' ",
          "foreground": "#f5919b",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "\udb84\udf75 <#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "foreground": "#907aa9",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": false,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 150
          },
          "template": "{{ .FormattedMs }}",
          "foreground": "#FFAB91",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\udb85\udf42",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
