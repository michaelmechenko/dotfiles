{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFE082",
          "style": "plain",
          "template": "@{{ .UserName }} \u279c",
          "type": "session"
        },
        {
          "foreground": "#9e7eff",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#CD4277",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#E8CC97>git(</>{{ .HEAD }}<#E8CC97>) </>",
          "type": "git"
        },
        {
          "foreground": "#FFAB91",
          "properties": {
            "always_enabled": false,
            "style": "austin",
            "threshold": 100
          },
          "style": "powerline",
          "template": "{{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "#CD4277",
          "style": "plain",
          "template": ">>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
