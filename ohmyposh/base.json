{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\udb85\udd45 ",
    "foreground": "#AEAED1",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "\udb85\udf42 {{ .UserName }} ",
          "foreground": "#AEAED1",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "cache_duration": "none",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": "\udb84\udf76 '{{ .Path }}' ",
          "foreground": "#DF8297",
          "type": "path",
          "style": "plain"
        },
        {
          "options": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#F5CC96>\udb84\udf75 '</>{{ .HEAD }}<#F5CC96>' </>",
          "foreground": "#BB9DBD",
          "type": "git",
          "style": "plain"
        },
        {
          "options": {
            "always_enabled": false,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 30000
          },
          "template": "{{ .FormattedMs }}",
          "foreground": "#8BA9C1",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "\udb83\udd72",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
